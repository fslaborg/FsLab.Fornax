{
    "$schema": "http://json.schemastore.org/template",
    "author": "Kevin Schneider",
    "classifications": [ "fslab", "web", "template" ],
    "name": "FsLab.Fornax.Template",
    "tags": {
        "language": "F#",
        "type": "project"
    },
    "identity": "FsLab.Fornax.Template",
    "shortName": "fslab-web",
    "sourceName": "FsLab.Fornax.Template",
    "symbols": {
        "base-url": {
            "description": "The base url of the deployed site, will be used when building the static htmls to prefix urls",
            "type": "parameter",
            "replaces": "{{BASE_URL}}",
            "defaultValue": ""
        },
        "site-title": {
            "description": "The title of the site, will be used in page metadata",
            "type": "parameter",
            "replaces": "{{SITE_TITLE}}",
            "defaultValue": ""
        },
        "site-description": {
            "description": "The description of the site, will be used in page metadata",
            "type": "parameter",
            "replaces": "{{SITE_DESCRIPTION}}",
            "defaultValue": ""
        },
    }
}